<?xml version="1.0" encoding="UTF-8"?>
<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- 渐变定义 -->
    <linearGradient id="primaryGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
    
    <linearGradient id="accentGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f093fb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f5576c;stop-opacity:1" />
    </linearGradient>
    
    <!-- 阴影滤镜 -->
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="2" dy="4" stdDeviation="3" flood-color="#000000" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- 背景圆形 -->
  <circle cx="200" cy="200" r="180" fill="url(#primaryGradient)" filter="url(#shadow)"/>
  
  <!-- 主要图标：流程图样式的任务流 -->
  <!-- 起始节点 -->
  <circle cx="120" cy="120" r="20" fill="#ffffff" stroke="url(#accentGradient)" stroke-width="3"/>
  <text x="120" y="127" text-anchor="middle" fill="url(#accentGradient)" font-family="Arial, sans-serif" font-size="16" font-weight="bold">S</text>
  
  <!-- 任务节点1 -->
  <rect x="90" y="170" width="60" height="40" rx="8" fill="#ffffff" stroke="url(#accentGradient)" stroke-width="2"/>
  <text x="120" y="193" text-anchor="middle" fill="url(#accentGradient)" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Task</text>
  
  <!-- 任务节点2 -->
  <rect x="190" y="170" width="60" height="40" rx="8" fill="#ffffff" stroke="url(#accentGradient)" stroke-width="2"/>
  <text x="220" y="193" text-anchor="middle" fill="url(#accentGradient)" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Flow</text>
  
  <!-- 决策节点 -->
  <polygon points="200,240 230,270 200,300 170,270" fill="#ffffff" stroke="url(#accentGradient)" stroke-width="2"/>
  <text x="200" y="275" text-anchor="middle" fill="url(#accentGradient)" font-family="Arial, sans-serif" font-size="10" font-weight="bold">AI</text>
  
  <!-- 结束节点 -->
  <circle cx="280" cy="320" r="20" fill="#ffffff" stroke="url(#accentGradient)" stroke-width="3"/>
  <text x="280" y="327" text-anchor="middle" fill="url(#accentGradient)" font-family="Arial, sans-serif" font-size="16" font-weight="bold">E</text>
  
  <!-- 连接线 -->
  <!-- S到Task -->
  <path d="M 120 140 L 120 170" stroke="url(#accentGradient)" stroke-width="3" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- Task到Flow -->
  <path d="M 150 190 L 190 190" stroke="url(#accentGradient)" stroke-width="3" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- Flow到AI -->
  <path d="M 220 210 L 200 240" stroke="url(#accentGradient)" stroke-width="3" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- AI到E -->
  <path d="M 215 285 L 260 305" stroke="url(#accentGradient)" stroke-width="3" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- 箭头标记 -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="url(#accentGradient)"/>
    </marker>
  </defs>
  
  <!-- 装饰性元素：齿轮 -->
  <g transform="translate(300, 100)">
    <circle cx="0" cy="0" r="25" fill="none" stroke="#ffffff" stroke-width="2" opacity="0.7"/>
    <circle cx="0" cy="0" r="15" fill="none" stroke="#ffffff" stroke-width="2" opacity="0.7"/>
    <!-- 齿轮齿 -->
    <rect x="-2" y="-30" width="4" height="10" fill="#ffffff" opacity="0.7"/>
    <rect x="-2" y="20" width="4" height="10" fill="#ffffff" opacity="0.7"/>
    <rect x="-30" y="-2" width="10" height="4" fill="#ffffff" opacity="0.7"/>
    <rect x="20" y="-2" width="10" height="4" fill="#ffffff" opacity="0.7"/>
    <rect x="-21" y="-21" width="4" height="10" fill="#ffffff" opacity="0.7" transform="rotate(45)"/>
    <rect x="17" y="-21" width="4" height="10" fill="#ffffff" opacity="0.7" transform="rotate(-45)"/>
    <rect x="-21" y="17" width="4" height="10" fill="#ffffff" opacity="0.7" transform="rotate(-45)"/>
    <rect x="17" y="17" width="4" height="10" fill="#ffffff" opacity="0.7" transform="rotate(45)"/>
  </g>
  
  <!-- 文档图标 -->
  <g transform="translate(80, 300)">
    <rect x="0" y="0" width="30" height="40" rx="3" fill="#ffffff" opacity="0.8"/>
    <rect x="25" y="0" width="10" height="10" fill="url(#accentGradient)" opacity="0.8"/>
    <line x1="5" y1="10" x2="25" y2="10" stroke="url(#accentGradient)" stroke-width="1" opacity="0.8"/>
    <line x1="5" y1="15" x2="25" y2="15" stroke="url(#accentGradient)" stroke-width="1" opacity="0.8"/>
    <line x1="5" y1="20" x2="20" y2="20" stroke="url(#accentGradient)" stroke-width="1" opacity="0.8"/>
  </g>
  
  <!-- 品牌文字 -->
  <text x="200" y="370" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="24" font-weight="bold">TaskFlow AI</text>
</svg>